{
  "version": "0.2.0",
  "configurations": [
  {
	  "name": "(gdb) Attach",
	  "type": "cppdbg",
	  "request": "attach",
	  "program": "${workspaceFolder}/a.out",
	  "processId": "${command:pickProcess}",
	  "MIMode": "gdb",
	  "setupCommands": [
		  {
			  "description": "Enable pretty-printing for gdb",
			  "text": "-enable-pretty-printing",
			  "ignoreFailures": true
		  }
	  ]
  },
  {
	  "name": "(gdb) Launch",
	  "type": "cppdbg",
	  "request": "launch",
	  "program": "${workspaceFolder}/a.out",
	  "args": [],
	  "stopAtEntry": false,
	  "cwd": "${fileDirname}",
	  "environment": [],
	  "externalConsole": false,
	  "MIMode": "gdb",
	  "setupCommands": [
		  {
			  "description": "Enable pretty-printing for gdb",
			  "text": "-enable-pretty-printing",
			  "ignoreFailures": true
		  }
	  ]
  },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
